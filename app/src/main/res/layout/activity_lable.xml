<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".unused.Label.LabelActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="648dp"
        android:orientation="vertical">
    </LinearLayout>

    <LinearLayout
        android:id="@+id/bottom_contain"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f5f5f5"
        android:layout_below="@id/divided_bottom">
        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <RadioButton
                android:id="@+id/home_bot_mainpage"
                android:checked="false"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="55dp"
                android:layout_width="28dp"
                android:layout_height="39dp"
                android:button="@drawable/home_bot_mainpage_selc">
            </RadioButton>
            <RadioButton
                android:id="@+id/home_bot_label"
                android:checked="true"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="57dp"
                android:layout_width="28dp"
                android:layout_height="39dp"
                android:button="@drawable/home_bot_label_selc">
            </RadioButton>
            <RadioButton
                android:id="@+id/home_bot_bluetooth"
                android:checked="false"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="53dp"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:button="@drawable/home_bot_bluetooth_selc">
            </RadioButton>
            <RadioButton
                android:id="@+id/home_bot_newbooks"
                android:checked="false"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="51dp"
                android:layout_width="wrap_content"
                android:layout_height="39dp"
                android:button="@drawable/home_bot_newbooks_selc">
            </RadioButton>
        </RadioGroup>
    </LinearLayout>
</LinearLayout>